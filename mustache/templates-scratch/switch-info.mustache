<!-- SISS-content-tabs not actually a class -->
			<div class="SISS-content-tabs"> 
			{{#navTabs}}
				<ul id="{{navTab-id}}" class="{{#navTabClassParams}}{{.}} {{/navTabClassParams}}">
				{{#navTab-listItems}}
				{{#isnotdrop}}
					<li class="{{listItemClass}}"><a href="#{{tabSection-id}}" data-toggle="tab">{{tabSection-title}}</a></li>
				{{/isnotdrop}}
				{{#isdrop}}
					<li class="dropdown"><a class="dropdown-toggle" data-toggle="dropdown" data-target="#" href="#">{{dropdownTitle}} <b class="caret"></b></a>
						<ul class="dropdown-menu">
						{{#listItems}}
							<li class="{{listItemClass}}"><a href="#{{tabSection-id}}" {{{data-toggle}}}>{{tabSection-title}}</a></li>
